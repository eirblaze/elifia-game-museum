{
  "name": "ekgm",
  "version": "1.0.0",
  "main": "index.js",
  "author": "eirblaze <eirblaze@live.jp>",
  "license": "MIT",
  "config": {
    "scss_src": "_src/sass/sv4.scss",
    "scss_map": "_src/map/style.css.map",
    "scss_out": "docs/css/sv4.css"
  },
  "scripts": {
    "all": "yarn sass && yarn js",
    "all-r": "yarn sass-r && yarn js-r",
    "js": "yarn webpack --mode=development",
    "js-r": "yarn webpack --mode=production",
    "js-w": "yarn webpack --mode=development -w",
    "sass": "yarn build-css-d && yarn autoprefixer",
    "sass-r": "yarn build-css-r && yarn autoprefixer-r",
    "sass-w": "yarn build-css-d && yarn build-css-d -- -w",
    "sass-n": "yarn nodemon-autoprefixer",
    "build-css":   "cross-conf-env node-sass --include-path scss npm_package_config_scss_src npm_package_config_scss_out",
    "build-css-d": "cross-conf-env yarn build-css -- --output-style expanded --source-map npm_package_config_scss_map --indent-type tab --indent-width 1 --source-comments",
    "build-css-r": "yarn build-css -- --output-style compressed",
    "autoprefixer": "cross-conf-env postcss --use autoprefixer --map -o npm_package_config_scss_out npm_package_config_scss_out",
    "autoprefixer-r": "cross-conf-env postcss --use autoprefixer --no-map -o npm_package_config_scss_out npm_package_config_scss_out",
    "nodemon-sass": "nodemon -e scss -x \"yarn build-css \"",
    "nodemon-autoprefixer": "nodemon -e scss -x \"yarn build-css && yarn autoprefixer \"",
    "test2": "yarn --version",
    "test": "node-sass --version"
  },
  "browserslist": [
    "last 2 versions",
    "not ie < 11",
    "Android >= 4",
    "iOS >= 9"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.3.1",
    "clean-css-cli": "^4.2.1",
    "cross-conf-env": "^1.1.2",
    "css-loader": "^2.1.1",
    "fstream": "1.0.12",
    "node-sass": "^4.7.2",
    "node-sass-import": "^1.1.1",
    "nodemon": "^1.17.4",
    "postcss": "^6.0.17",
    "postcss-cli": "^5.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue": "^2.6.6",
    "vue-loader": "^15.6.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
